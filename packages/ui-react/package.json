{
  "name": "@spscommerce/ui-react",
  "version": "4.0.3",
  "description": "@spscommerce/ui-react React component",
  "main": "lib/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build:watch": "npx nodemon -w src -x 'rm .babelrc &> /dev/null; nwb build-react-component --no-demo'",
    "build": "nwb build-react-component --no-demo",
    "clean": "nwb clean-module",
    "test": "jest --no-cache --env=jsdom --coverage",
    "test:watch": "jest --no-cache --env=jsdom --coverage --watchAll"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-router": "4.x",
    "react-router-dom": "4.x"
  },
  "devDependencies": {
    "babel-jest": "23.0.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "expect": "23.1.0",
    "jest-canvas-mock": "1.0.2",
    "jest-cli": "23.1.0",
    "jest-fetch-mock": "1.6.3",
    "jest-localstorage-mock": "2.2.0",
    "mock-local-storage": "1.0.5",
    "nwb": "0.21.x",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-test-renderer": "16.4.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "dependencies": {
    "classnames": "2.2.5",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-router-dom": "4.3.1"
  }
}
